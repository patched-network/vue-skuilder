{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "ESNext",
    "moduleResolution": "node",
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "docs/test/**/*.ts",
    "cypress/**/*.ts"
  ]
}
