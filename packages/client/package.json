{
  "name": "@vue-skuilder/client",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.19",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "rimraf dist && tsc && rollup --bundleConfigAsCjs -c",
    "dev": "rollup --bundleConfigAsCjs -c -w",
