{
    "name": "@vue-skuilder/cli",
    "publishConfig": {
        "access": "public"
    },
    "version": "0.1.4",
    "type": "module",
    "description": "CLI scaffolding tool for vue-skuilder projects",
    "bin": {
        "skuilder": "./dist/cli.js"
    },
    "main": "dist/cli.js",
    "types": "dist/cli.d.ts",
    "exports": {
        ".": {
            "types": "./dist/cli.d.ts",
            "import": "./dist/cli.js"
        }
    },
    "scripts": {
        "build": "rm -rf dist && tsc",
        "dev": "tsc --watch",
        "lint": "npx eslint .",
        "lint:fix": "npx eslint . --fix",
        "lint:check": "npx eslint . --max-warnings 0"
    },
    "keywords": [
        "cli",
        "scaffolding",
        "vue",
        "skuilder",
        "education",
        "course"
    ],
    "dependencies": {
        "@vue-skuilder/db": "workspace:*",
        "@vue-skuilder/standalone-ui": "^0.1.4",
        "chalk": "^5.3.0",
        "commander": "^11.0.0",
        "fs-extra": "^11.2.0",
        "inquirer": "^9.2.0",
        "pouchdb": "^9.0.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.0",
        "@types/inquirer": "^9.0.0",
        "@types/node": "^24.0.0",
        "typescript": "~5.7.2"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "packageManager": "yarn@4.6.0"
}
