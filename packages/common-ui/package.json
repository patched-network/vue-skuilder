{
  "name": "@vue-skuilder/common-ui",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.5",
  "main": "./dist/common-ui.umd.js",
  "module": "./dist/common-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/common-ui.es.js",
      "require": "./dist/common-ui.umd.js"
    },
    "./style": "./dist/assets/index.css",
    "./styles": "./dist/assets/index.css"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && yarn build-types",
    "build-types": "tsc --project tsconfig.types.json",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "test:unit": "vitest run",
    "cypress:open": "cypress open --component",
    "cypress:run": "cypress run --component"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.2",
    "@mdi/font": "^7.3.67",
    "@vojtechlanka/vue-tags-input": "^3",
    "@vue-skuilder/common": "workspace:*",
    "@vue-skuilder/db": "workspace:*",
    "highlight.js": "^11.0.1",
    "marked": "^15.0.6",
    "moment": "^2.29.4",
    "mousetrap": "^1.6.5",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vuetify": "^3.7.0"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^6.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/pouchdb": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.4.0",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^14.2.1",
    "cypress-vite": "^1.6.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^25.0.1",
    "sass": "^1.83.0",
    "typescript": "~5.7.2",
    "vite": "^6.0.9",
    "vitest": "^3.0.5"
  }
}
