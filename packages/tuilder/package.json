{
  "name": "@vue-skuilder/tuilder",
  "version": "0.1.7-0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "bin": "./dist/cli.js",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js"
  },
  "dependencies": {
    "@vue-skuilder/common": "workspace:*",
    "@vue-skuilder/courses": "workspace:*",
    "@vue-skuilder/db": "workspace:*",
    "ink": "^5.0.1",
    "ink-markdown": "^1.0.4",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.3",
    "@types/yargs": "^17.0.32",
    "typescript": "~5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@4.6.0",
  "stableVersion": "0.1.7"
}
